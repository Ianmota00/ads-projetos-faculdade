package atividade03;

public class Aluno {
	    private Aluno aluno;
	    private String nome;
	    private int idade;

	    public Aluno(String nome, int idade) {
	        this.nome = nome;
	        this.idade = idade;
	    }

	    public String getNome() {
	        return nome;
	    }

	    public int getIdade() {
	        return idade;
	    }

		public Aluno getAluno() {
			return aluno;
		}

		public void setAluno(Aluno aluno) {
			this.aluno = aluno;
		}

	   
	    
	


}
