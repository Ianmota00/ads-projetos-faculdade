package atividade03;

public class Main {

	public static void main(String[] args) {
		ListaDuplamenteEncadeada lista = new ListaDuplamenteEncadeada();
		// Cadastrar 5 alunos
        lista.inserirFim(new Aluno("Alice", 20));
        lista.inserirFim(new Aluno("Bruno", 21));
        lista.inserirFim(new Aluno("Carla", 22));
        lista.inserirFim(new Aluno("Diego", 23));
        lista.inserirFim(new Aluno("Eduarda", 24));

        
        lista.inserirInicio(new Aluno("Fernanda", 25));

        
        lista.inserirFim(new Aluno("Gabriel", 26));

       
        System.out.println("Alunos na lista:");
        lista.exibirLista();
    }

	}


